    agent {
        label 'jenkins-agent'
    }
    tools {
        jdk 'Java17'
        maven 'Maven3'
    }
    stages {
        stage('Checkout from scm'){
            steps{
                git branch: 'main', credentialsId: 'github-cred', url: 'https://github.com/gogulanikhil/devops-full-project'
            }
        }
    }
}